Algoritmo "Deposito"

Var saldoInicial, saldo, deposito: inteiro

Inicio
      saldoInicial<- 100
      escreva("Informe o valor do seu deposito")
      leia(deposito)

      saldo<- saldoInicial + deposito
      escreval("Saldo inicial: ", saldoInicial)
      escreval("Seu depósito foi de: ", deposito)
      escreval("O saldo atual da sua conta é: ",saldo)

Fimalgoritmo